<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <Preference
        android:key="@string/pref_key_backups"
        android:title="@string/backups"
        app:iconSpaceReserved="false" />

    <Preference
        android:key="@string/pref_key_security"
        android:title="@string/security_and_privacy"
        app:iconSpaceReserved="false" />

    <Preference
        android:title="@string/contacts"
        app:iconSpaceReserved="false">
        <intent
            android:targetClass="com.flowcrypt.email.ui.activity.settings.ContactsSettingsActivity"
            android:targetPackage="@string/application_id" />
    </Preference>

    <Preference
        android:key="@string/pref_key_keys"
        android:title="@string/keys"
        app:iconSpaceReserved="false" />

    <Preference
        android:key="@string/pref_key_attester"
        android:title="@string/attester"
        app:iconSpaceReserved="false" />

    <Preference
        android:key="@string/pref_key_notification"
        android:title="@string/notifications"
        app:iconSpaceReserved="false" />

    <Preference
        android:key="@string/pref_key_server_settings"
        android:title="@string/server_settings"
        app:iconSpaceReserved="false"
        app:isPreferenceVisible="false" />

    <Preference
        android:key="@string/pref_key_legal"
        android:title="@string/legal"
        app:iconSpaceReserved="false" />

    <Preference
        android:key="@string/pref_key_experimental"
        android:title="@string/experimental"
        app:iconSpaceReserved="false" />
</PreferenceScreen>
