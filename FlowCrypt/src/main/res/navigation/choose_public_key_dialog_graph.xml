<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/choose_public_key_dialog_graph"
    app:startDestination="@id/choosePublicKeyDialogFragment">

    <dialog
        android:id="@+id/choosePublicKeyDialogFragment"
        android:name="com.flowcrypt.email.ui.activity.fragment.dialog.ChoosePublicKeyDialogFragment"
        android:label="ChoosePublicKeyDialogFragment"
        tools:layout="@layout/fragment_send_user_public_key">
        <argument
            android:name="requestKey"
            app:argType="string" />
        <argument
            android:name="email"
            app:argType="string" />
        <argument
            android:name="choiceMode"
            app:argType="integer" />
        <argument
            android:name="titleResourceId"
            android:defaultValue="-1"
            app:argType="integer" />
        <argument
            android:name="returnResultImmediatelyIfSingle"
            android:defaultValue="false"
            app:argType="boolean" />
    </dialog>
</navigation>
