<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/decrypt_attachment_dialog_graph"
    app:startDestination="@id/decryptAttachmentDialogFragment">

    <dialog
        android:id="@+id/decryptAttachmentDialogFragment"
        android:name="com.flowcrypt.email.ui.activity.fragment.dialog.DecryptAttachmentDialogFragment"
        tools:layout="@layout/fragment_decrypt_attachment">
        <argument
            android:name="requestKey"
            app:argType="string" />
        <argument
            android:name="attachmentInfo"
            app:argType="com.flowcrypt.email.api.email.model.AttachmentInfo" />
        <argument
            android:name="requestCode"
            android:defaultValue="0"
            app:argType="integer" />
    </dialog>
</navigation>
