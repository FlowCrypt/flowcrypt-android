<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/import_additional_private_keys_graph"
    app:startDestination="@id/importAdditionalPrivateKeysFragment">

    <fragment
        android:id="@+id/importAdditionalPrivateKeysFragment"
        android:name="com.flowcrypt.email.ui.activity.fragment.ImportAdditionalPrivateKeysFragment"
        tools:layout="@layout/fragment_import_additional_private_keys">
        <argument
            android:name="requestKey"
            app:argType="string" />
        <argument
            android:name="accountEntity"
            app:argType="com.flowcrypt.email.database.entity.AccountEntity" />
        <argument
            android:name="bundle"
            android:defaultValue="@null"
            app:argType="android.os.Bundle"
            app:nullable="true" />
    </fragment>
</navigation>
